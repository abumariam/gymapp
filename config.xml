<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.frigatesoft.gym"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>Oman GYM</name>

  <description>
      GYM Membership Management App
  </description>

  <author href="https://www.frigatesoft.com" email="abdelkawy@frigatesoft.com">
      Project Team
  </author>

  <!-- all platforms -->
  <preference name="phonegap-version" value="cli-6.5.0" />
<!--   <preference name="fullscreen" value="true" /> -->  
  <content src="index.html"/>
  <access origin="*"/>
  <!-- iOS only -->
  <preference name="target-device" value="universal" />
  <preference name="prerendered-icon" value="true" />
  <preference name="detect-data-types" value="true" />
  <preference name="exit-on-suspend" value="true" />
  <preference name="deployment-target" value="7.0" />

  <!-- Android only -->
  <preference name="android-build-tool" value="ant|gradle" />
  <preference name="android-minSdkVersion" value="10" />
  <preference name="android-maxSdkVersion" value="15" />
  <preference name="android-targetSdkVersion" value="12" />
  <preference name="android-installLocation" value="auto" />
  <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />



<icon src="android_icons/icon.png" platform="ios" width="180" height="180" />

<icon src="android_icons/icon.png" platform="android" width="180" height="180" />

<icon src="ldpi.png" platform="android" qualifier="ldpi" />
<icon src="android_icons/mdpi.png" platform="android" qualifier="mdpi" />
<icon src="android_icons/hdpi.png" platform="android" qualifier="hdpi" />
<icon src="android_icons/xhdpi.png" platform="android" qualifier="xhdpi" />
<icon src="android_icons/xxhdpi.png" platform="android" qualifier="xxhdpi" />
<icon src="android_icons/xxxhdpi.png" platform="android" qualifier="xxxhdpi" />
<icon src="android_icons/fr-xxhdpi.png" platform="android" qualifier="fr-xxhdpi" />

</widget>